import List as List

let width = 800,
    height = 480,
    canvas = new_canvas width height

draw_text "Lines!" 200 0 canvas

seed (ticks)

List.map (List.range 0 100) (i) -> do
  let x0 = random width,
      y0 = random height,
      x1 = random width,
      y1 = random height
  draw_line x0 y0 x1 y1 canvas
end
