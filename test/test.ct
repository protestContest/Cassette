

def (integers-from n) do
  n | \ -> (integers-from n + 1)
end

let ints = (integers-from 10)

def (nth-stream s n) do
  if n == 0 do
    head s
  else
    nth-stream ((tail s)) n - 1
  end
end

IO.inspect (head ints)
IO.inspect (nth-stream ints 52)

cond do
  ints == nil -> :ok
  true        -> :also_ok
end
