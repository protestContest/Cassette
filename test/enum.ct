module Enum

def (trim-end-to item enum index) cond do
  index == 0 -> index
  enum.(index - 1) == item -> index
  true -> trim-end-to item enum index - 1
end

def (trim-end item enum index) cond do
  index == 0 -> index
  enum.(index - 1) != item -> index
  true -> trim-end item enum index - 1
end
