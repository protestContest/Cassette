def (fizzbuzz n) do
  def (div3 n) (Math.rem n 3) == 0
  def (div5 n) (Math.rem n 5) == 0

  def (loop m) do
    cond do
      m % 3 == 0 and m % 5 == 0   -> print "FizzBuzz"
      m % 3 == 0                  -> print "Fizz"
      m % 5 == 0                  -> print "Buzz"
      true                        -> print m
    end

    if m < n do
      loop m + 1
    end
  end

  loop 1
end

fizzbuzz 100
